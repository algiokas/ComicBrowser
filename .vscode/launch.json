{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug API",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}\\api\\bin\\www",
        "cwd": "${workspaceFolder}\\api"
      },{
        "type": "chrome",
        "request": "launch",
        "name": "Launch Client",
        "port": 9001,
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/dist",
        "sourceMaps": true
      }
    ]
  }